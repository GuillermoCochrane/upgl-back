                      },
                      {
                        "stubID": 2,
                        "order": 2,
                        "available": true,
                        "text": "Implica el desarrollo de sistemas que pueden interpretar y comprender el contenido visual, como imágenes y videos",
                        "content": "plain"
                      }
                    ]
